    "xspeed = 2\n",
    "logo = None\n",
    "\n",
    "def setup():\n",
    "    global logo\n",
    "    size(800,600)\n",
    "    logo = load_image('images/animation_and_motion/dvd-logo.png')\n",
    "\n",
    "def draw():\n",
    "    global x, xspeed, logo\n",
    "    background('#000000')\n",
